#: import ListAdapter kivy.adapters.listadapter.ListAdapter 
#: import main main

[ScreenHeading@BoxLayout]:
	size_hint_y: 14
	size_hint_x: ctx.size_hint_x
	background_color: 0.169, 0.20, 0.275, 1
    canvas:
        Color:
            rgba: self.background_color
        Rectangle:
            pos: self.pos
            size: self.size
	Label:
		font_name: "Johnston.ttf"
		font_size: 35
		text_size: root.width, None
		size: self.texture_size
		color: (1, 1, 1, 1)
		valign: "center"
		halign: "center"
		text: ctx.text
	
[BasicLabel@Label]:
	font_name: "Open Sans.ttf"
	text_size: root.width, None
	size: self.texture_size
	color: (0, 0, 0, 1)
	halign: ctx.halign
	text: ctx.text
	font_size: ctx.font_size
	size_hint_y: ctx.size_hint_y
	size_hint_x: ctx.size_hint_x
	


AppScreenManager:
	LoginScreen:
	SignupScreen:
	RecentNewsScreen:
	ArticleScreen:
		id: screenarticle
	AccountScreen:
	ChangePasswordScreen:
	FilterScreen:
	ListScreen:
		id: screenlist
	ProfileScreen:
		id: screenprofile
	DateScreen:
		id: screendate
	TimeScreen:
		id: screentime

<LoginScreen>:
	name: "login"
	BoxLayout:
		orientation: "vertical"
		ScreenHeading:
			text: "Login"
			size_hint_x: 1
		GridLayout:
			rows: 5
			size_hint_y: 64
			Label:
				size_hint_y: 20
			BoxLayout:
				orientation: "vertical"
				size_hint_y: 25
				padding: [20,0]
				BasicLabel:
					halign: "left"
					font_size: 22
					size_hint_y: 6
					size_hint_x: 1
					text: "Email:"
				TextInput:
					id: EmailText     
					multiline: False
					font_size: 20
					font_name: "Open Sans.ttf"
					size_hint_y: 4
			BoxLayout:
				orientation: "vertical"
				size_hint_y: 25
				padding: [20,0]
				BasicLabel:
					halign: "left"
					font_size: 22
					size_hint_y: 6
					size_hint_x: 1
					text: "Password:"
				TextInput:
					id: PasswordText  
					multiline: False
					font_size: 20
					font_name: "Open Sans.ttf"
					password: True
					size_hint_y: 4
			Label:
				size_hint_y: 30
		GridLayout:
			rows: 5
			size_hint_y: 22
			Button:
				size_hint_y: 33
				text: "<< AUTO LOGIN >>"
				on_release: root.FileChecker() 
				background_normal: "2c9c91.png"
				background_down: "2c9c91.png"
				font_name: "Open Sans.ttf"
				font_size: 23
			Label:
				size_hint_y: 0.7
			Button:
				size_hint_y: 33
				text: "<< LOGIN >>"
				on_release: root.InputChecker(EmailText.text, PasswordText.text)   
				background_normal: "2c9c91.png"
				background_down: "2c9c91.png"
				font_name: "Open Sans.ttf"
				font_size: 23
			Label:
				size_hint_y: 0.7
			Button:
				size_hint_y: 33
				text: "<< SIGN-UP >>"
				on_release: app.root.current = "signup"
				background_normal: "2c9c91.png"
				background_down: "2c9c91.png"
				font_name: "Open Sans.ttf"
				font_size: 23
			
